<div>
  <% if(!username){ %>
    <form action="/login" method="POST">
      <input id="username" type="text" name="username" style="width: 300px">
      <input type="submit" value="Submit">
    </form>
  <% } else{ %>
    <form action="/logout" method="POST">
    <dev><%= username %>
      <button>Delete</button>
    </dev>
    </form>
  <% } %>
</div>